{
  "manifest_version": 2,
  "offline_enabled": true,
  "default_locale": "en",
  "name": "Stratus",
  "description": "A clean and fast new tab page",
  "version": "1.0.0",
  "homepage_url": "https://github.com/dineshkhadka/stratus",
  "browser_action": {
    "default_icon": "icons/128x128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  }
}
